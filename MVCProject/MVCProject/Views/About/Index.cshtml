@using EntityLayer.Concrete
@model List<About>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>Haqqında datalar</h2>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Haqqımızda başlığı</th>
        <th>Aktiv et</th>
        <th>Pasiv et</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.AboutİD</td>
            <td>Başlıq</td>
            <td><a href="#" class="btn btn-info">Aktiv et</a></td>
            <td><a href="#" class="btn btn-danger">Pasiv et</a></td>
        </tr>
    }
</table>

@Html.Action("AboutPartial","About")
